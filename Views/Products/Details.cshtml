@model Products

@{
    ViewBag.HideHeader = false;
}

<head>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" rel="stylesheet">
</head>

<div class="container mt-5" style="font-size: 14px;">

    @Html.Partial("ProductInfo", Model)

    <div>
        @Html.Partial("SectionInfo", Model)

        <div class="tab-content mt-3" id="myTabContent">

            @Html.Partial("ReviewsInfo", Model)

            @Html.Partial("ReviewModelInfo", Model)

            @Html.Partial("QuestionInfo", Model)

            @Html.Partial("QuestionModelInfo", Model)

        </div>
    </div>
</div>